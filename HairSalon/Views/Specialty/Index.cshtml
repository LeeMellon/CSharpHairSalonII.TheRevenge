@{
  Layout = "_Layout";
}
<h1>Hair Salon</h1>
<h2>Specialties</h2>
@foreach(var specialty in Model)
{
  <h4><a href="/specialty/@specialty.GetId()">@specialty.GetName()</a>  |<a href="/specialty/@specialty.GetId()/delete">Delete This Specialty</a></h4>

}
<form class="" action="/specialty/new" method="get">
  <button type="submit" name="button">Add New Specialty</button>
</form>

<h4><a href="/">Return to Home</a></h4>
